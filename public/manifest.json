{
    "name": "better copy title",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon32.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/",
                "https://www.google.com/"
            ],
            "js": [
                "content_scripts.js"
            ]
        }
    ],
    "permissions":[
        "activeTab",
        "storage"
    ]
}